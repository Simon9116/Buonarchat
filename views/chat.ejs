<!doctype html>
<html lang="en">
<head>
    <%- include("partials/head.ejs") %>
    <title>Chat</title>
</head>
<body>
    <ul id="messages">
    <% for(let message of messages) { %>
        <li><%= message %></li>
    <%}%>
    </ul>
    <form id="send-message-form" action="/" method="GET">
        <input name="text" type="text" placeholder="Messaggio">

        <input type="submit" value="Invia">
    </form>
</body>
</html>